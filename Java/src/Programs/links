// Step 1: Retrieve column values
List<WebElement> elementsBeforeSort = getDriver()
    .findElements(By.xpath("//div[@id='program']//span[@class='ag-cell-value']"));

// Step 2: Extract text from each element
List<String> columnValues = new ArrayList<>();
for (WebElement element : elementsBeforeSort) {
    columnValues.add(element.getText().trim()); // trim to avoid leading spaces
}

// Step 3: Create a sorted copy of the column values
List<String> sortedColumnValues = new ArrayList<>(columnValues);
Collections.sort(sortedColumnValues, String.CASE_INSENSITIVE_ORDER); // or use Comparator.naturalOrder()

// Step 4: Compare original and sorted list
if (columnValues.equals(sortedColumnValues)) {
    System.out.println("Column is sorted in ascending order.");
} else {
    System.out.println("Column is NOT sorted in ascending order.");
}